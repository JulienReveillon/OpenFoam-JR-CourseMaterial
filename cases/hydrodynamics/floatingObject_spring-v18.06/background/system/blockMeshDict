FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      blockMeshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

scale   1;

vertices
(
	(0 0 0)		//0
	(20 0 0)	//1
	(20 0.1 0)	//2
	(0 0.1 0)	//3	
	(0 0 10)	//4
	(20 0 10)	//5
	(20 0.1 10)	//6
	(0 0.1 10)	//7
);

blocks
(
    hex (0 1 2 3 4 5 6 7) (200 1 100) simpleGrading (1 1 1)
);

edges
(
);

boundary
(
    // Dummy patch to trigger overset interpolation before any other
    // bcs
    oversetPatch
    {
        type overset;
        faces ();
    }
    inlet
    {
        type patch;
        faces
        (
            (0 3 7 4)
        );
    }
    outlet
    {
        type patch;
        faces
        (
	    (1 2 6 5)
        );
    }
    wall
    {
        type wall;
        faces
        (
            (0 1 2 3)
        );
    }
    atmosphere
    {
        type patch;
        faces
        (
            (4 5 6 7)
        );
    }
);

mergePatchPairs
(
);

